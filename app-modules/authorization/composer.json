{
    "name": "canyon-gbs/aiding-app-authorization",
    "description": "",
    "type": "library",
    "version": "1.0",
    "license": "proprietary",
    "require": {
        "filament/filament": "^3.2",
        "spatie/laravel-permission": "^5.10"
    },
    "autoload": {
        "psr-4": {
            "AidingApp\\Authorization\\": "src/",
            "AidingApp\\Authorization\\Tests\\": "tests/",
            "AidingApp\\Authorization\\Database\\Factories\\": "database/factories/",
            "AidingApp\\Authorization\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "AidingApp\\Authorization\\Providers\\AuthorizationServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
