{
    "name": "canyon-gbs/advising-app-integration-ai",
    "description": "",
    "type": "library",
    "version": "1.0",
    "license": "proprietary",
    "require": {
        "filament/filament": "^3.0.0",
        "openai-php/client": "^0.8.4",
        "spatie/laravel-settings": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "AidingApp\\IntegrationAI\\": "src/",
            "AidingApp\\IntegrationAI\\Tests\\": "tests/",
            "AidingApp\\IntegrationAI\\Database\\Factories\\": "database/factories/",
            "AidingApp\\IntegrationAI\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "AidingApp\\IntegrationAI\\Providers\\IntegrationAIServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
