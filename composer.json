        "canyon-gbs/aiding-app-integration-google-recaptcha": "*",
        "canyon-gbs/aiding-app-integration-microsoft-clarity": "*",
        "canyon-gbs/aiding-app-integration-twilio": "*",
        "canyon-gbs/aiding-app-interaction": "*",
        "canyon-gbs/aiding-app-inventory-management": "*",
        "canyon-gbs/aiding-app-knowledge-base": "*",
        "canyon-gbs/aiding-app-notification": "*",
        "canyon-gbs/aiding-app-portal": "*",
        "canyon-gbs/aiding-app-contact": "*",
        "canyon-gbs/aiding-app-report": "*",
        "canyon-gbs/aiding-app-service-management": "*",
        "canyon-gbs/aiding-app-survey": "*",
        "canyon-gbs/aiding-app-task": "*",
        "canyon-gbs/aiding-app-team": "*",
        "canyon-gbs/aiding-app-theme": "*",
        "canyon-gbs/aiding-app-timeline": "*",
        "canyon-gbs/aiding-app-webhook": "*",
        "composer/composer": "^2.7",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.1",
        "filament/spatie-laravel-settings-plugin": "^3.1",
        "friendsofcat/opensearch-migrations": "^2.0",
        "friendsofcat/opensearch-scout-driver": "^2.1",
        "friendsofcat/opensearch-scout-driver-plus": "^2.1",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.2",
        "internachi/modular": "^2.0",
        "kirschbaum-development/eloquent-power-joins": "^3.5",
        "laravel/framework": "10.46",
        "laravel/octane": "^2.3",
        "laravel/sanctum": "^3.3",
        "laravel/scout": "^10.8",
        "laravel/socialite": "^5.12",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.4",
        "lastdragon-ru/lara-asp-graphql": "^5.3",
        "league/flysystem-aws-s3-v3": "^3.24",
        "league/oauth2-client": "^2.7",
        "livewire/livewire": "^3.4",
        "maatwebsite/excel": "^3.1",
        "microsoft/microsoft-graph": "^1.109",
        "mll-lab/graphql-php-scalars": "^6.2",
        "nuwave/lighthouse": "^6.33",
        "owen-it/laravel-auditing": "^13.6",
        "saade/filament-fullcalendar": "^3.1",
        "shuvroroy/filament-spatie-laravel-health": "^2.0",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/microsoft-azure": "^5.1",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-data": "^3.11",
        "spatie/laravel-health": "v1.24.1",
        "spatie/laravel-medialibrary": "^10.15",
        "spatie/laravel-multitenancy": "^3.1",
        "spatie/laravel-settings": "^3.3",
        "spiral/roadrunner-cli": "^2.6",
        "spiral/roadrunner-http": "^3.4",
        "sqids/sqids": "^0.4.1",
        "staudenmeir/eloquent-has-many-deep": "^1.19",
        "stechstudio/filament-impersonate": "^3.5",
        "talkroute/message-segment-calculator": "^1.0",
        "tapp/filament-timezone-field": "^3.0",
        "tpetry/laravel-postgresql-enhanced": "^0.34",
        "twilio/sdk": "^7.15"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.15",
        "brianium/paratest": "^7.2",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.49",
        "larastan/larastan": "^2.7",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.11",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.1",
        "pestphp/pest-plugin-livewire": "^2.1",
        "phpunit/phpunit": "^10.1",
        "rector/rector": "^0.19.0",
        "serversideup/spin": "v2.0.0-beta-3",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.32",
        "worksome/request-factories": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "refresh-database": [
            "@php artisan tenants:artisan \"migrate:fresh --database=tenant\"",
            "@php artisan tenants:artisan \"db:seed --database=tenant\""
        ],
        "php-cs-format": [
            "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix -v --config=./php-cs-fixer.php"
        ],
        "php-cs-format-dryrun": [
            "@php-cs-format --dry-run --stop-on-violation"
        ],
        "php-cs-clear-cache": [
            "rm -f ./.php-cs-fixer.cache"
        ],
        "prettier": [
            "./node_modules/.bin/prettier \"resources/**/*{.css,.blade.php,.js}\" \"app-modules/**/resources/**/*{.css,.blade.php,.js}\" \"graphql/*.graphql\" \"app-modules/**/graphql/*.graphql\" --config ./.prettierrc.json --ignore-path ./.prettierignore --cache"
        ],
        "prettier-format": [
            "@prettier --write"
        ],
        "prettier-format-dryrun": [
            "@prettier --check"
        ],
        "clear-prettier-cache": [
            "rm -f ./node_modules/.cache/prettier/.prettier-cache"
        ],
        "format-dryrun": [
            "@php-cs-format-dryrun",
            "@prettier-format-dryrun"
        ],
        "format": [
            "@php-cs-format",
            "@prettier-format"
        ],
        "generate-helper-files": [
            "@php artisan app:generate-helper-files",
            "./vendor/bin/rector",
            "@php-cs-format"
        ],
        "lint": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "checks": [
            "@format-dryrun",
            "@lint"
        ],
        "ci": [
            "rm -rf ./vendor",
            "composer install"
        ],
        "generate-api-docs": [
            "./node_modules/.bin/prettier \"graphql/*.graphql\" \"app-modules/**/graphql/*.graphql\" --config ./.prettierrc.json --ignore-path ./.prettierignore --cache --write",
            "@php artisan api:print-schema",
            "npm run api-docs:generate"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": [
                "nesbot/carbon"
            ]
        },
        "google/apiclient-services": [
            "Oauth2",
            "Calendar"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "path",
            "url": "app-modules/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
