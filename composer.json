    "name": "canyongbs/aidingapp",
    "type": "project",
    "description": "Aiding App™ by Canyon GBS™ is a Conversational AI and CRM ecosystem solution built specifically for postsecondary education use, adding features, functionality, and sector capabilities.",
    "keywords": [
        "higher",
        "education",
        "academic",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "8.2.*",
        "ext-gd": "*",
        "ext-pdo": "*",
        "awcodes/filament-tiptap-editor": "^3.2.17",
        "aws/aws-php-sns-message-validator": "^1.9",
        "aws/aws-sdk-php": "^3.300",
        "barryvdh/laravel-debugbar": "^3.9",
        "bvtterfly/model-state-machine": "^0.3.0",
